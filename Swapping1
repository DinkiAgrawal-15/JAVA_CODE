import java.util.*;
class Main{
    
       

    public static void main(String arg[]){
        int z;
        Scanner sc=new Scanner(System.in);
        System.out.println("Enter 1");
       int  a=sc.nextInt();
        
        System.out.println("Enter 2");
              int   b=sc.nextInt();

       System.out.println("Before swap"+a+" "+b);
        int t;
       t=a;
       a=b;
       b=t;
       System.out.println("After swapping"+a+" "+b);
    }
}
    
