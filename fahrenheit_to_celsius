import java.util.*;
 class Main {
    public static void main(String a[]){
        float c;
        float f;
        Scanner sc=new Scanner(System.in);
        System.out.println("Enter the temp in Fehrenheit");

        f=sc.nextFloat();
        c=(float)((f-32)/1.8);
        System.out.print("Temperature in fahrenheit="+c);
     }

    
    
}
