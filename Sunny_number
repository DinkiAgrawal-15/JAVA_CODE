import java.util.*;
class Main{
    public static void main(String a[]){
        double x;
        
        Scanner sc=new Scanner(System.in);
        System.out.println("Enter the number");
       int  num=sc.nextInt();
       x=Math.sqrt(num+1);
       if((int)x==x){
           System.out.println(num+"is sunny number");
           
       }else{
           System.out.println(num+"is not sunny");
       }
       
       
        
    }
}
